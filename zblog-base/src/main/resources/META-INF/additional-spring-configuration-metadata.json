      "defaultValue": 3600
    },
    {
      "name": "deploy.type",
      "type": "java.lang.String",
      "sourceType": "com.github.stazxr.zblog.base.config.properties.DeployProperties",
      "description": "部署类型：single - 单机部署；multi - 多节点部署.",
      "defaultValue": "single"
